{
  "version": 2,
  "builds": [
    {
      "src": "api1/api1.py",
      "use": "@vercel/python"
    },
    {
      "src": "api2/api2.py",
      "use": "@vercel/python"
    },
    {
      "src": "api3/api3.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api1/(.*)",
      "dest": "/api1/api1.py"
    },
    {
      "src": "/api2/(.*)",
      "dest": "/api2/api2.py"
    },
    {
      "src": "/api3/(.*)",
      "dest": "/api3/api3.py"
    }
  ]
}
